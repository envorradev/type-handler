{
    "name": "envorradev/type-handler",
    "repositories": [
        {
            "type": "path",
            "url": "../TaylorNetwork/class-scanner"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/collections": "^9",
        "taylornetwork/class-scanner": "*"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "ramsey/composer-repl": "*",
        "phpstan/phpstan": "*",
        "illuminate/support": "^9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Envorra\\TypeHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Envorra\\TypeHandler\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sam Taylor",
            "email": "15961687+samueljtaylor@users.noreply.github.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "ramsey/composer-repl": true
        }
    }
}
